grant select
 , insert
 , update
 , delete
 , create
  on kiaspdotnetframeworkdb_x.*
  to 'ki-kiaspdotnetframework-x'@'localhost' identified by '~ki-kiaspdotnetframework-x-password~'
;
--
-- Must grant PROCESS priv to primary user to include SHOW ENGINE INNODB
-- STATUS output in MySQL-related exception reports.
--
grant process
  on *.*
  to 'ki-kiaspdotnetframework-x'@'localhost'
