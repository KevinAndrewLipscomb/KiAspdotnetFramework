grant select
 , insert
 , update
 , delete
 , create
  on kiaspdotnetframeworkdb_d.*
  to 'ki-kiaspdotnetframework-d'@'localhost' identified by '~ki-kiaspdotnetframework-d-password~'
;
--
-- Must grant PROCESS priv to primary user to include SHOW ENGINE INNODB
-- STATUS output in MySQL-related exception reports.
--
grant process
  on *.*
  to 'ki-kiaspdotnetframework-d'@'localhost'